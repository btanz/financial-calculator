//- planning-retire-template
script(id='planning-retire-results-2-template', type='text/x-handlebars-template')
  div.ibox.float-e-margins
    div.ibox-title
      h5
        i.fa.fa-bar-chart-o.fa-lg.text-navy &nbsp
        | {{obj.title}}
    div.ibox-content
      table.table.table-striped
        thead
          tr
            | {{#with obj.header}}
            th(style='text-align: left; padding-right: 30px;') {{{this.[0]}}}
            th(style='text-align: right; padding-left: 30px;') {{{this.[1]}}}
            th(style='text-align: right; padding-right: 30px;') {{{this.[2]}}}
            | {{/with}}
        tbody
          | {{#each obj.body}}
          tr
            td(style='text-align: left; padding-right: 30px;') {{numFormat this.[0] 0}}
            td(style='text-align: right; padding-left: 30px;') {{numFormat this.[1] this.digits}}
            td(style='text-align: right; padding-right: 30px;') {{numFormat this.[2] this.digits}}
          | {{/each}}